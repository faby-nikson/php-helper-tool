{
    "name": "f-dumas/php-helper-tool",
    "description": "PHP Helper tool.",
    "require": {
        "php": ">8.1",
        "ext-json": "*",
        "ext-zlib": "*",
        "symfony/console": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "FDTool\\PhpHelper\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "m6web/php-cs-fixer-config": "^1.3",
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^5.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin/"
    }
}
